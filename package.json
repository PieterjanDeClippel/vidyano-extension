{
  "name": "vidyano-extension",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && npx nx build",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "npx nx serve",
    "watch:tsc": "tsc --noEmit --watch --project packages/vidyano-extension/tsconfig.app.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "19.8.0",
    "@nx/eslint": "19.8.0",
    "@nx/eslint-plugin": "19.8.0",
    "@nx/jest": "19.8.0",
    "@nx/js": "19.8.0",
    "@nx/node": "^19.8.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "~18.16.9",
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.19.12",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "nx": "19.8.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "tslib": "^2.3.0",
    "vscode": "^1.1.37"
  }
}
